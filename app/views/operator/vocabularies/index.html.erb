<p id="notice"><%= notice %></p>

<h1>Operator Vocabularies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_vocabularies.each do |a| %>
      <tr>
        <td><%= link_to 'Show', operator_vocabularies_path %></td>
        <td><%= link_to 'Edit', edit_operator_vocabulary_path(a) %></td>
        <td><%= link_to 'Destroy', operator_vocabularies_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

